  ylab("number of unique BCs with mutation data") + 
  xlab("source environment") +
  scale_y_continuous(limits = c(-3,60), breaks = seq(0,60,10)) +
  annotate(geom = "text", label = paste0(mut_t3$all_adapted), x = mut_t3$source, y = -2, size = 2)
