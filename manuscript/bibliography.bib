	Mesh = {Adaptation, Biological; Analysis of Variance; Biological Evolution; Cell Culture Techniques; Environment; Genetic Fitness; Genotype; Glucose; Phenotype; Pseudomonas fluorescens; Selection, Genetic; Xylose},
	Month = {Nov},
	Number = {11},
