	Mesh = {ATP-Binding Cassette Transporters; Bacterial Proteins; Base Sequence; Evolution, Molecular; Host Factor 1 Protein; Maltose-Binding Proteins; Molecular Sequence Data; Porins; Pseudomonas fluorescens; Selection, Genetic},
	Pages = {4076},
	Pmid = {24912567},
	Pst = {epublish},
	Title = {Adaptive synonymous mutations in an experimentally evolved Pseudomonas fluorescens population},
	Volume = {5},
	Year = {2014},
