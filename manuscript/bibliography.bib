	Month = {Aug},
	Number = {2},
	Pages = {270-83},
	Pmid = {22766936},
	Pst = {ppublish},
	Title = {Spatial structure of ecological opportunity drives adaptation in a bacterium},
	Volume = {180},
	Year = {2012},
